    "electron-json-storage": "^4.0.2",
    "electron-squirrel-startup": "^1.0.0",
    "moment": "^2.21.0",
    "react": "^15.4.1",
    "react-dom": "^15.4.1",
    "react-hot-loader": "^3.0.0-beta.6",
    "react-redux": "^5.0.7",
    "react-router": "^4.2.0",
